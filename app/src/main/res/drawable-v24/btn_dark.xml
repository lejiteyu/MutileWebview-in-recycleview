<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true"  >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <!--<stroke android:width="0dp" android:color="#bebebe" />-->
            <solid android:color="@color/BreatheColor1"></solid>
        </shape>
    </item>

    <item android:state_pressed="true"   >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <!--<stroke android:width="0dp" android:color="#bebebe" />-->
            <solid android:color="@color/teal_200"></solid>
        </shape>
    </item>

    <item android:state_enabled="false"
    >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="0dp" android:color="#BCC8C8C8" />
            <solid android:color="#795C5C5C"></solid>
        </shape>
    </item>

    <item android:state_focused="true"    >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="0dp" android:color="#673AB7" />
            <solid android:color="@color/higlight_blue"></solid>
        </shape>
    </item>
    <item >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="2dp" android:color="#000000" />
<!--            <solid android:color="@color/white"></solid>-->
        </shape>
    </item>
</selector>